<Window x:Class="engener.Summary"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:engener"
        mc:Ignorable="d"
        Title="Summary" Height="500" Width="800" Background="Red" MinWidth="500" MinHeight="400">
    <Grid Background="Black" Margin="5px">
        <Label x:Name="Info" Content="Label" HorizontalAlignment="Center" Margin="0,129,0,0" VerticalAlignment="Top" Foreground="White"/>
        <Button x:Name="Confirm" Content="Zapisz regułę" HorizontalAlignment="Left" Margin="204,361,0,0" VerticalAlignment="Top" Click="Confirm_Click"/>
        <Button x:Name="Cancel" Content="Anuluj" HorizontalAlignment="Left" Margin="492,361,0,0" VerticalAlignment="Top" Click="Cancel_Click"/>

    </Grid>
</Window>
