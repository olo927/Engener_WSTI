<Window x:Class="engener.AddCategory"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:engener"
        mc:Ignorable="d"
        Title="Dodaj Kategorie" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="CategoryTextBox" HorizontalAlignment="Left" Margin="348,171,0,0"  TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="AddCategoryButton" Content="Dodaj" HorizontalAlignment="Left" Margin="296,256,0,0" VerticalAlignment="Top" Click="AddCategoryButton_Click"/>
        <Button x:Name="Cancel" Content="Anuluj" HorizontalAlignment="Left" Margin="473,256,0,0" VerticalAlignment="Top" Click="Cancel_Click"/>

    </Grid>
</Window>
